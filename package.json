{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "build": "vite build",
    "generate:outfits": "node scripts/generate-outfits.js",
    "lint": "eslint .",
    "lint:a11y": "eslint . --ext .tsx,.ts --no-eslintrc --config eslint.config.js | grep jsx-a11y || echo 'No accessibility violations found'",
    "test:a11y": "./scripts/accessibility-check.sh",
    "test:keyboard": "node scripts/test-keyboard-navigation.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "verify:local": "node scripts/verify-deployment.js",
    "verify:production": "SITE_URL=https://your-app.netlify.app node scripts/verify-deployment.js",
    "audit:continuous": "node scripts/continuous-audit.js",
    "audit:pre-commit": "node scripts/pre-commit-audit.js",
    "audit:setup-hooks": "node scripts/setup-git-hooks.js",
    "audit:schedule": "node scripts/audit-scheduler.js",
    "audit:trends": "node scripts/audit-scheduler.js trends",
    "audit:status": "node scripts/audit-scheduler.js status",
    "audit:validate": "node scripts/validate-audit-implementation.js"
  },
  "dependencies": {
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "netlify-cli": "^23.8.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.3.6",
    "vitest": "^3.2.4"
  }
}
